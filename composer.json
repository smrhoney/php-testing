{
  "name": "smrhoney/php-testing",
  "description": "PHP Testing Tutorials",
  "minimum-stability": "dev",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Shawn Rhoney",
      "email": "smrhoney@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "5.6"
    }
  },
  "require-dev": {
    "atoum/atoum": "3.3.0",
    "php-coveralls/php-coveralls": "2.1.x-dev",
    "phpunit/phpunit": "5.7.27"
  },
  "autoload": {
    "psr-4": {
      "SMRhoney\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SMRhoney\\Test\\": "test/"
    }
  }
}
